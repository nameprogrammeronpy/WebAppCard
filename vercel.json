{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/card-of-day", "destination": "/card-of-day.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Content-Security-Policy", "value": "frame-ancestors 'self' https://web.telegram.org https://telegram.org https://*.telegram.org" }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}