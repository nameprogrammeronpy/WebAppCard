{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/card-of-day", "destination": "/card-of-day.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}